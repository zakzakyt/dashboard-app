{
    "name": "e2e",
    "type": "module",
    "scripts": {
      "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
      "test:welcomepage": "npm run test -- welcomepage.test.js",
      "test:layout": "npm run test -- welcomepage.test.js --testNamePattern='layout test'",
      "test:sidebar": "npm run test -- welcomepage.test.js --testNamePattern='Sidebar test'",
      "test:main": "npm run test -- welcomepage.test.js --testNamePattern='Main content test'"
    },
    "devDependencies": {
      "jest": "^29.7.0",
      "jest-dev-server": "^11.0.0",
      "jest-puppeteer": "^11.0.0",
      "puppeteer": "^24.7.1"
    }
  }
